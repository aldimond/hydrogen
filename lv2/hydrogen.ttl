@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<http://hydrogen-music.org/plugins/hydrogen>
	a lv2:Plugin ,
		lv2:InstrumentPlugin;
		lv2:project <http://lv2plug.in/ns/lv2> ;
	doap:maintainer [
	  foaf:name     "Hydrogen team" ;
	  ];
	doap:name "Hydrogen";
	doap:license <https://opensource.org/licenses/GPL-2.0> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:extensionData state:interface ,
		work:interface ;
	ui:ui <http://hydrogen-music.org/plugins/hydrogen#ui>;
	lv2:port [
		a lv2:AudioPort ,
		lv2:OutputPort ;
		lv2:index 0;
		lv2:symbol "out_l" ;
		lv2:name "Out (L)"
	] , [
		a lv2:AudioPort ,
		lv2:OutputPort ;
		lv2:index 1;
		lv2:symbol "out_r" ;
		lv2:name "Out (R)"
	] , [
		a lv2:InputPort ,
		atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:designation lv2:control ;
		lv2:index 2;
		lv2:symbol "control" ;
		lv2:name "Control"
    	] .


<http://hydrogen-music.org/plugins/hydrogen#ui>
	a ui:Qt5UI ;
	lv2:requiredFeature urid:map ;
	lv2:optionalFeature ui:requestValue ;
	lv2:extensionData ui:showInterface .
